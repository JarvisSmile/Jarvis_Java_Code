		this.biginteger = biginteger;
	}
	public H04_BigIntegerClass(String s)
	{
		this.biginteger=s.toCharArray();
		this.BI=new int[biginteger.length];
		for(int i=0;i<biginteger.length;i++) {
			BI[i]=(int)biginteger[i]-48;
		}
	}
