package com.jarvis.base;

import java.util.Random;

public class homework3 {
	public static void Array(String s) {
		String []p=s.split(" ");	
		char[] a1=p[0].toCharArray();
		for(int i=a1.length-1;i>=0;i--) {
			System.out.print(a1[i]);
		}

		System.out.print(" ");
		char[] a2=p[1].toCharArray();
		for(int i=a2.length-1;i>=0;i--) {
			System.out.print(a2[i]);
		}
	}
	public static void Snake_array(int []a) {
		int n=a.length;
		for(i;i<n;) {
			for(j;;)
		}
	}
	public static void M_Num(int n) {
		double []a=new double[n];
		Random random1 = new Random(n);
		for(int i=0;i<n;i++) {
			a[i]=Math.abs(random1.nextDouble()*100/1);
		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String a="Hello World";
//		Array(a);
		M_Num(6);
	}

}
